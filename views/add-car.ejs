<%- include('_header'); %>
<title>ADD CAR</title>
<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1 class="m-0">Dashboard</h1>
        </div><!-- /.col -->
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="#">Home</a></li>
            <li class="breadcrumb-item active">Dashboard v1</li>
          </ol>
        </div><!-- /.col -->
      </div><!-- /.row -->
    </div><!-- /.container-fluid -->
  </div>
  <!-- /.content-header -->

  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <!-- Small boxes (Stat box) -->
      <form method="POST" action="/add-car"> 
        <div class="form-row">
          <div class="form-group col-md-6">
            <label for="inputEmail4">Car Brand</label>
            <input type="text" class="form-control" id="inputEmail4" placeholder="Brand" name="brand">
          </div>
          <div class="form-group col-md-6">
            <label for="inputPassword4">Plate-ID</label>
            <input type="text" class="form-control" id="inputPassword4" placeholder="Plate-ID" name="plate">
          </div>
        </div>
        <div class="form-group">
          <label for="inputAddress">Color</label>
          <input type="text" class="form-control" id="inputAddress" placeholder="color" name="color">
        </div>
        <div class="form-row">
          <div class="form-group col-md-12">
            <label for="inputCity">Class</label>
            <input type="text" class="form-control" id="inputCity" name="class">
          </div>
        </div>
        <div class="form-row">
          <div class="form-group col-md-6">
            <label for="inputCity">Garage Latitude</label>
            <input type="text" class="form-control" id="inputCity" name="cor1">
          </div>
          <div class="form-group col-md-6">
            <label for="inputCity">Garage Longitude</label>
            <input type="text" class="form-control" id="inputCity" name="cor2">
          </div>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
      <!-- /.row -->
    
      <!-- /.row (main row) -->
    </div><!-- /.container-fluid -->
  </section>
  <%- include('_footer'); %>