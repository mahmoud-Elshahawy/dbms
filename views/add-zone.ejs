<%- include('_header'); %>
<title>ADD Zone</title>
<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1 class="m-0">Dashboard</h1>
        </div><!-- /.col -->
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="#">Home</a></li>
            <li class="breadcrumb-item active">Dashboard v1</li>
          </ol>
        </div><!-- /.col -->
      </div><!-- /.row -->
    </div><!-- /.container-fluid -->
  </div>
  <!-- /.content-header -->

  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <!-- Small boxes (Stat box) -->
      <form method="POST" action="/add-zone"> 
        <div class="form-row">
          <div class="form-group col-md-12">
            <label for="inputEmail4">Zone Name</label>
            <input type="text" class="form-control" id="inputEmail4" placeholder="Zone" name="zone_name">
          </div>
        </div>
        <div class="form-row">
        <div class="form-group col-md-6">
          <label for="inputAddress">Longtitude 1</label>
          <input type="text" class="form-control" id="inputAddress" placeholder="Longtitude" name="p1">
        </div>
        <div class="form-group col-md-6">
          <label for="inputAddress2">Latitude 1</label>
          <input type="text" class="form-control" id="inputAddress2" placeholder="Latitude" name="p2">
        </div></div>
        <div class="form-row">
          <div class="form-group col-md-6">
            <label for="inputAddress">Longtitude 2</label>
            <input type="text" class="form-control" id="inputAddress" placeholder="Longtitude" name="p3">
          </div>
          <div class="form-group col-md-6">
            <label for="inputAddress2">Latitude 2</label>
            <input type="text" class="form-control" id="inputAddress2" placeholder="Latitude" name="p4">
          </div></div>
          <div class="form-row">
            <div class="form-group col-md-6">
              <label for="inputAddress">Longtitude 3</label>
              <input type="text" class="form-control" id="inputAddress" placeholder="Longtitude" name="p5">
            </div>
            <div class="form-group col-md-6">
              <label for="inputAddress2">Latitude 3</label>
              <input type="text" class="form-control" id="inputAddress2" placeholder="Latitude" name="p6">
            </div></div>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="inputAddress">Longtitude 4</label>
                <input type="text" class="form-control" id="inputAddress" placeholder="Longtitude" name="p7">
              </div>
              <div class="form-group col-md-6">
                <label for="inputAddress2">Latitude 4</label>
                <input type="text" class="form-control" id="inputAddress2" placeholder="Latitude" name="p8">
              </div></div>
        </div>
        <center>
        <button type="submit" class="btn btn-success btn-lg">SUBMIT</button></center>
      </form>
      <!-- /.row -->
    
      <!-- /.row (main row) -->
    </div><!-- /.container-fluid -->
  </section>
  <%- include('_footer'); %>